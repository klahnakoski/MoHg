{
	"use_cache": true,
	"hg": {
		"url": "https://hg.mozilla.org"
	},
	"repo": {
		"host": "http://localhost",
		"port": 9200,
		"index": "repo",
		"type": "revision",
		"typed": false,
		"timeout": 300,
		"debug": false,
		"limit_replicas": false
	},
	"debug":{
		"trace":true,
		"constants":{
			"mo_http.http.default_headers": {"Referer": "https://github.com/klahnakoski/mo-hg"},
			"mo_hg.hg_mozilla_org.MAX_DIFF_SIZE":1000000,
			"mo_hg.hg_mozilla_org.DEBUG": true,
			"mo_hg.hg_mozilla_org.WAIT_AFTER_CACHE_MISS": 0
		},
		"logs":[
			{"log_type":"console"}

		]
	}
}
